                </Path>
                <Ellipse Grid.Column="3" HorizontalAlignment="Center" Height="15" Margin="0" Stroke="White" StrokeThickness="3" VerticalAlignment="Center" Width="15" Fill="White" Grid.ColumnSpan="3"/>
            </Grid>
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                <Button Content="Player" HorizontalAlignment="Center" Margin="0" VerticalAlignment="Center" Width="51.444" Height="51" Style="{DynamicResource RoundedButton}" Background="#FF2C6BEB">
                    <Button.Effect>
                        <DropShadowEffect BlurRadius="20" Direction="269"/>
                    </Button.Effect>
                </Button>
                <StackPanel Grid.Column="1" VerticalAlignment="Center">
                    <Button Content="Player" HorizontalAlignment="Center" Margin="0 0 0 50" VerticalAlignment="Center" Width="51.444" Height="51" Style="{DynamicResource RoundedButton}" Background="#FF2C6BEB" Grid.Column="1">
                        <Button.Effect>
                            <DropShadowEffect BlurRadius="20" Direction="269"/>
                        </Button.Effect>
