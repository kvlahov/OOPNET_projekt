                    <Button Content="Player" HorizontalAlignment="Center" Margin="0" VerticalAlignment="Center" Width="51.444" Height="51" Style="{DynamicResource RoundedButton}" Background="#FF2C6BEB" Grid.Column="1">
                        <Button.Effect>
                            <DropShadowEffect BlurRadius="20" Direction="269"/>
                        </Button.Effect>
                    </Button>
                </StackPanel>

            </Grid>
        </Grid>
    </Border>
</UserControl>
