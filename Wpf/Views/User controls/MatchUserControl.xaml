                <Path Grid.Column="6" Height="244.286" Stroke="White" Grid.ColumnSpan="3" StrokeThickness="3" Data="M1.7177835,1.5 L125.86079,1.5 L125.86079,242.786 L1.7177835,242.786 z M1.5,62.786 L46.5,62.786 L46.5,182.072 L1.5,182.072 z M128.21639,69.286 C145.65313,69.286 159.78839,92.791051 159.78839,121.786 159.78839,150.78095 145.65313,174.286 128.21639,174.286" VerticalAlignment="Center" RenderTransformOrigin="0.5,0.5" Margin="0,0,-3,0" HorizontalAlignment="Right">
                    <Path.RenderTransform>
                        <TransformGroup>
                            <ScaleTransform ScaleY="1" ScaleX="-1"/>
                            <SkewTransform AngleY="0" AngleX="0"/>
                            <RotateTransform Angle="0"/>
                            <TranslateTransform/>
                        </TransformGroup>
                    </Path.RenderTransform>
                </Path>
                <Path Data="M26.5,1.5 C40.307119,1.5 51.5,12.692881 51.5,26.5" HorizontalAlignment="Left" Height="28" Stretch="Fill" Stroke="White" StrokeThickness="3" VerticalAlignment="Top" Width="28" RenderTransformOrigin="0.5,0.5" Margin="-2,-2,0,0" >
                    <Path.RenderTransform>
                        <TransformGroup>
                            <ScaleTransform ScaleY="-1" ScaleX="1"/>
                            <SkewTransform AngleY="0" AngleX="0"/>
                            <RotateTransform Angle="0"/>
                            <TranslateTransform/>
                        </TransformGroup>
                    </Path.RenderTransform>
                </Path>
                <Path Data="M26.5,1.5 C40.307119,1.5 51.5,12.692881 51.5,26.5" HorizontalAlignment="Left" Height="28" Stretch="Fill" Stroke="White" StrokeThickness="3" VerticalAlignment="Bottom" Width="28" RenderTransformOrigin="0.5,0.5" Margin="-2,0,0,-2" >
                    <Path.RenderTransform>
                        <TransformGroup>
                            <ScaleTransform ScaleY="1" ScaleX="1"/>
                            <SkewTransform AngleY="0" AngleX="0"/>
                            <RotateTransform Angle="0"/>
                            <TranslateTransform/>
                        </TransformGroup>
                    </Path.RenderTransform>
                </Path>
                <Path Data="M26.5,1.5 C40.307119,1.5 51.5,12.692881 51.5,26.5" HorizontalAlignment="Right" Height="28" Stretch="Fill" Stroke="White" StrokeThickness="3" VerticalAlignment="Top" Width="28" RenderTransformOrigin="0.5,0.5" Grid.Column="8" Margin="0,-2,-2,0" >
                    <Path.RenderTransform>
                        <TransformGroup>
                            <ScaleTransform ScaleY="-1" ScaleX="-1"/>
                            <SkewTransform AngleY="0" AngleX="0"/>
                            <RotateTransform Angle="0"/>
                            <TranslateTransform/>
                        </TransformGroup>
                    </Path.RenderTransform>
                </Path>
                <Path Data="M26.5,1.5 C40.307119,1.5 51.5,12.692881 51.5,26.5" HorizontalAlignment="Right" Height="28" Stretch="Fill" Stroke="White" StrokeThickness="3" VerticalAlignment="Bottom" Width="28" RenderTransformOrigin="0.5,0.5" Grid.Column="8" Margin="0,0,-2,-2" >
